{
  "name": "@feseeker/ni",
  "version": "0.0.1",
  "packageManager": "pnpm@8.0.0",
  "description": "Use the right package manager",
  "author": "feseeker",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "ni": "bin/ni.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "esno src/commands/ni.ts",
    "nr": "esno src/commands/nr.ts",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "release": "bumpp && npm publish",
    "lint": "eslint .",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@posva/prompts": "^2.4.4",
    "@types/ini": "^1.3.31",
    "@types/node": "^20.3.3",
    "@types/which": "^3.0.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.38.0",
    "esno": "^0.16.3",
    "execa": "^7.1.1",
    "fast-glob": "^3.3.0",
    "find-up": "^6.3.0",
    "ini": "^4.1.1",
    "kleur": "^4.1.5",
    "terminal-link": "^3.0.0",
    "unbuild": "^1.2.1",
    "vitest": "^0.30.1",
    "which": "^3.0.1"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
